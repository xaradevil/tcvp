module		avcodec
name		"TCVP/codec/avcodec"
version		0.1.1
tc2version	0.6.0
sources		avcodec.c encode.c avc.h
postinit	avc_init

TCVP {
	filter "decoder/audio/mpeg" {
		alias "decoder/audio/mp2"
		alias "decoder/audio/mp3"
		alias "decoder/audio/wmav2"
		alias "decoder/audio/wmav1"
		alias "decoder/audio/ac3"
		alias "decoder/audio/mace3"
		alias "decoder/audio/mace6"
		alias "decoder/audio/adpcm-ms"
		alias "decoder/audio/adpcm-ima-qt"
		new avc_new
		probe avc_probe_audio
		packet DATA avc_decaudio
		flush avc_flush
	}

	filter "decoder/video/mpeg" {
		alias "decoder/video/mpeg2"
		alias "decoder/video/h263"
		alias "decoder/video/h263p"
		alias "decoder/video/h263i"
		alias "decoder/video/h264"
		alias "decoder/video/rv10"
		alias "decoder/video/mjpeg"
		alias "decoder/video/mjpegb"
		alias "decoder/video/mpeg4"
		alias "decoder/video/msmpeg4v1"
		alias "decoder/video/msmpeg4v2"
		alias "decoder/video/msmpeg4v3"
		alias "decoder/video/wmv1"
		alias "decoder/video/wmv2"
		alias "decoder/video/vp3"
		alias "decoder/video/svq1"
		alias "decoder/video/svq3"
		alias "decoder/video/indeo3"
		new avc_new
		probe avc_probe_video
		packet DATA avc_decvideo
		flush avc_flush
	}

	filter "encoder/video/mpeg" {
		new avc_mpeg_enc_new
		packet DATA avc_encvid
		probe avc_encvideo_probe
		flush avc_encvid_flush
	}

	filter "encoder/video/mpeg2" {
		new avc_mpeg2_enc_new
		packet DATA avc_encvid
		probe avc_encvideo_probe
		flush avc_encvid_flush
	}

	filter "encoder/video/mpeg4" {
		new avc_mpeg4_enc_new
		packet DATA avc_encvid
		probe avc_encvideo_probe
		flush avc_encvid_flush
	}

	filter "encoder/video/msmpeg4v3" {
		new avc_msmpeg4v3_enc_new
		packet DATA avc_encvid
		probe avc_encvideo_probe
		flush avc_encvid_flush
	}

	filter "encoder/video/msmpeg4v2" {
		new avc_msmpeg4v2_enc_new
		packet DATA avc_encvid
		probe avc_encvideo_probe
		flush avc_encvid_flush
	}
}
