# -*- tcconf -*-
bind 'q'	'*'	'quit()'
bind 'Up'	'video'	'send(TCVP_SEEK, 60000, 1)'
bind 'Down'	'video'	'send(TCVP_SEEK, -60000, 1)'
bind 'Right'	'video'	'send(TCVP_SEEK, 10000, 1)'
bind 'Left'	'video'	'send(TCVP_SEEK, -10000, 1)'
bind 'space'	'video'	'send(TCVP_PAUSE)'
bind 'Next'	'*'	'send(TCVP_PL_NEXT)'
bind 'Prior'	'*'	'send(TCVP_PL_PREV)'
bind 'Play'	'*'	'send(TCVP_PL_START); send(TCVP_START)'
bind 'Pause'	'*'	'send(TCVP_PAUSE)'
bind 'Stop'	'*'	'send(TCVP_PL_STOP); send(TCVP_CLOSE)'

