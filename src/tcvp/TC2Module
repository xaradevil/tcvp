module		tcvp_core
name		"TCVP"
version		0.1.4
tc2version	0.4.0
sources		tcvp.c event.c

import		"stream"	"open"
import		"stream"	"play"
import		"timer"		"new"
import		"driver/timer"	"new"
import		"tcvp/event"	"alloc"
import		"tcvp/event"	"new"
require		"filter"

TCVP {
	module "tcvp/core" t_new t_init
	event control	TCVP_KEY	NULL key_alloc NULL NULL
	event control	TCVP_OPEN	te_open open_alloc open_ser open_deser
	event control	TCVP_OPEN_MULTI	te_openm open_multi_alloc NULL NULL
	event control	TCVP_START	t_start NULL NULL NULL
	event control	TCVP_STOP	t_stop NULL NULL NULL
	event control	TCVP_PAUSE	te_pause NULL NULL NULL
	event control 	TCVP_SEEK	te_seek seek_alloc seek_ser seek_deser
	event control	TCVP_CLOSE	te_close NULL NULL NULL
	event timer	TCVP_TIMER	NULL timer_alloc timer_ser timer_deser
	event status	TCVP_STATE	NULL state_alloc state_ser state_deser
	event status	TCVP_LOAD	NULL load_alloc load_ser load_deser
	event status	TCVP_STREAM_INFO NULL NULL NULL NULL
	event control	TCVP_QUERY	te_query NULL NULL NULL
	event control	TCVP_BUTTON	NULL button_alloc NULL NULL
}

option		default_profile%s="play"
option		verbose%i
