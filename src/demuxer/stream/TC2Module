import		"tcvp/event"	"send"
import		"tcvp/event"	"get"
import		"URL"		"open"
require		"demux"

TCVP {
	event TCVP_STATE
	event TCVP_LOAD
}

option		buffer%i=64
Number of packets to read ahead for each stream.

option		max_probe%i=1024
Maximum number of packets read when probing stream parameters.

option		magic_size%i=24
Number of bytes to check for magic signatures.

option	*suffix suffix%s demuxer%s muxer%s
Mapping of filename suffixes to formats if detection fails.
